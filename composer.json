{
    "name": "victoryoalli/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": [
        "victoryoalli",
        "shoppingcart",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Yoalli",
            "email": "victoryoalli@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0",
        "illuminate/session": "^7.0",
        "illuminate/events": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "VictorYoalli\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VictorYoalli\\Shoppingcart\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VictorYoalli\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "VictorYoalli\\Shoppingcart\\CartFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}